# Environment variables for act (local GitHub Actions testing)
# Copy this to .env.act.local and update with your values

# Required for tests
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/relayforge_test
ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef
NODE_ENV=test

# OAuth test credentials (use test values)
COOKIE_SECRET=test-cookie-secret-minimum-32-characters-long
JWT_SECRET=test-jwt-secret-minimum-32-characters-long
GOOGLE_CLIENT_ID=test-google-client-id
GOOGLE_CLIENT_SECRET=test-google-client-secret

# Optional: Turbo cache (leave empty for local testing)
TURBO_TOKEN=
TURBO_TEAM=